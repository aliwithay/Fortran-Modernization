##----some of the interested variables for plotting/analysis----##
1) flow fields in out.ncf:
    ncdump -h out.ncf #properties of the netcdf file
dimensions:
        X = 64 ; # number of grid points in x, y, z direction
        Y = 64 ;
        Z = 64 ;
        T = 2 ; #number of time points
variables:
        float TIMES(T) ; 
        float U(T, Y, Z, X) ; #u,v,w are velocity
        float V(T, Y, Z, X) ; 
        float W(T, Y, Z, X) ;
        float ZX(T, Y, Z, X) ; #zx,zy,zz are vorticity field
        float ZY(T, Y, Z, X) ;
        float ZZ(T, Y, Z, X) ;
        float TH(T, Y, Z, X) ; #th is temperature field
        float QV(T, Y, Z, X) ; #qv is water vapor mixing ratio field 
}

